<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/sage_green2" />
        </shape>
    </item>
    <item android:top="2dp" android:left="2dp" android:right="2dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <stroke android:width="3dp" android:color="@color/puce" android:dashWidth="8dp" android:dashGap="6dp" />
            <padding android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp" />
        </shape>
    </item>
</layer-list>